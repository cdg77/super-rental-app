<%= render 'layouts/errors', :object => @city %>
<div class="container">
  <%= form_for @city do |f| %>
    <div class="form-group well">
      <div class="form-inline">
        <%= f.label :name %>
        <%= f.text_field :name %>
      </div>
      <div class="form-inline">
        <%= f.label :state %>
        <%= f.text_field :state %>
      </div>
      <div class="form-inline">
        <%= f.label :country %>
        <%= f.text_field :country %>
      </div>

      <div class="form-inline">
        <%= f.submit  %>
      </div>
    </div>
  <% end %>
</div>
